        </script>

        <!--Static Files-->
        
        <link href="/css/style.css" rel="stylesheet"> 
        <link href="/css/main.css" rel="stylesheet"> 
        <link href="/favicon.png" rel="icon" type="image/png" />
        <script src="/js/navbar.js" defer></script>
